# Auto detect text files and perform LF normalization
* text=auto
*.mdb filter=lfs diff=lfs merge=lfs -text
*.parcel-cache[[:space:]]filter=lfs[[:space:]]diff=lfs[[:space:]]merge=lfs[[:space:]]-text filter=lfs diff=lfs merge=lfs -text
*.Loopizo/.parcel-cache/data.mdb[[:space:]]filter=lfs[[:space:]]diff=lfs[[:space:]]merge=lfs[[:space:]]-text filter=lfs diff=lfs merge=lfs -text
.parcel-cache/** filter=lfs diff=lfs merge=lfs -text
